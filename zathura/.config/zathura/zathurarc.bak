include colors.zat

set statusbar-v-padding         15
set statusbar-h-padding         75
set font "Monoisome 8"
set pages-per-row 1
set statusbar-basename          true
set page-padding 1

map u scroll half-up
map d scroll half-down
map D toggle_page_mode
map r reload
map R rotate
map K zoom in
map J zoom out
map <Button3> follow_link

